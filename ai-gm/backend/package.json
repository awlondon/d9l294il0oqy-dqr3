{
  "name": "ai-gm-backend",
  "version": "0.1.0",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --enable-source-maps dist/server.js",
    "dev": "ts-node --esm src/server.ts",
    "test": "node --loader ts-node/esm src/test.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --check \"src/**/*.{ts,tsx,js}\""
  },
  "dependencies": {
    "express": "^4.19.2",
    "mqtt": "^5.7.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.10",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
